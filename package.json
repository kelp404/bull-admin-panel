    "eslint-config-xo": "0.40.0",
    "eslint-config-xo-react": "0.26.0",
    "eslint-config-xo-space": "0.32.0",
    "eslint-plugin-react": "7.29.3",
    "eslint-plugin-react-hooks": "4.3.0",
    "file-loader": "6.2.0",
