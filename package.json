{
  "name": "bull-admin-panel",
  "version": "1.0.1",
  "description": "An admin panel of Bull based on WebSocket.",
  "main": "index.js",
  "scripts": {
    "start": "BULL_ADMIN_PANEL_DEBUG=true concurrently --kill-others \"node_modules/nodemon/bin/nodemon.js example/server.js --watch example --watch lib\" \"node_modules/webpack-dev-server/bin/webpack-dev-server.js\"",
    "build": "rm -rf lib/frontend/dist && NODE_ENV=production node_modules/webpack/bin/webpack.js",
    "ncu": "node_modules/.bin/npm-check-updates",
    "eslint": "node_modules/eslint/bin/eslint.js ./ --ignore-path .gitignore --ignore-pattern 'lib/frontend/dist/'",
    "changelog": "node_modules/conventional-changelog-cli/cli.js -n node_modules/@kelp404/changelog.config",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "node_modules/jest/bin/jest.js --config __tests__/backend/jest.config.js",
    "test:frontend": "node_modules/jest/bin/jest.js --config __tests__/frontend/jest.config.js"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelp404/bull-admin-panel.git"
  },
  "keywords": [
    "bull",
    "queue",
    "job-queue"
  ],
  "author": "Kelp <kelp@phate.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kelp404/bull-admin-panel/issues"
  },
  "homepage": "https://github.com/kelp404/bull-admin-panel#readme",
  "dependencies": {
    "bull": "4.8.3",
    "express": "4.17.3",
    "fastest-validator": "1.12.0",
    "handlebars": "4.7.7",
    "path-to-regexp": "6.2.0",
    "query-string": "7.1.1",
    "ws": "8.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@fortawesome/fontawesome-free": "6.1.0",
    "@kelp404/changelog.config": "0.1.0",
    "@kelp404/react-infinite-scroller": "1.2.5",
    "babel-loader": "8.2.4",
    "bootstrap": "4.5.3",
    "capybara-router": "2.8.0",
    "classnames": "2.3.1",
    "compression-webpack-plugin": "4.0.1",
    "concurrently": "7.1.0",
    "config": "3.3.7",
    "conventional-changelog-cli": "2.2.2",
    "css-loader": "5.0.1",
    "dayjs": "1.11.0",
    "eslint": "8.14.0",
    "eslint-config-xo": "0.40.0",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-space": "0.32.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "file-loader": "6.2.0",
    "history": "5.3.0",
    "jest": "27.5.1",
    "jest-environment-jsdom": "27.5.1",
    "jest-environment-node": "27.5.1",
    "jquery": "3.6.0",
    "mini-css-extract-plugin": "1.6.2",
    "node-sass": "6.0.1",
    "nodemon": "2.0.15",
    "npm-check-updates": "12.5.8",
    "nprogress": "0.2.0",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "pubsub-js": "1.9.4",
    "react": "16.13.1",
    "react-bootstrap": "1.3.0",
    "react-dom": "16.13.1",
    "react-flip-move": "3.0.4",
    "react-test-renderer": "16.13.1",
    "redis": "4.0.6",
    "sass-loader": "10.2.1",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3"
  }
}
