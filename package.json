{
  "name": "bull-admin-panel",
  "version": "0.9.0",
  "description": "An admin panel of Bull based on WebSocket.",
  "main": "index.js",
  "scripts": {
    "start": "BULL_ADMIN_PANEL_DEBUG=true concurrently --kill-others \"node_modules/nodemon/bin/nodemon.js example/server.js --watch example --watch lib\" \"node_modules/webpack-dev-server/bin/webpack-dev-server.js\"",
    "build": "rm -rf lib/frontend/dist && NODE_ENV=production node_modules/webpack/bin/webpack.js",
    "ncu": "node_modules/.bin/npm-check-updates",
    "eslint": "node_modules/eslint/bin/eslint.js ./ --ignore-path .gitignore --ignore-pattern 'lib/frontend/dist/'",
    "changelog": "node_modules/conventional-changelog-cli/cli.js -p angular",
    "test": "node_modules/jest/bin/jest.js --coverage __tests__",
    "coveralls": "node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info"
  },
  "files": [
    "index.js",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelp404/bull-admin-panel.git"
  },
  "keywords": [
    "bull",
    "queue",
    "job-queue"
  ],
  "author": "Kelp <kelp@phate.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kelp404/bull-admin-panel/issues"
  },
  "homepage": "https://github.com/kelp404/bull-admin-panel#readme",
  "dependencies": {
    "bull": "4.7.0",
    "express": "4.17.3",
    "fastest-validator": "1.12.0",
    "handlebars": "4.7.7",
    "path-to-regexp": "6.2.0",
    "query-string": "6.13.7",
    "ws": "7.5.7"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@fortawesome/fontawesome-free": "6.0.0",
    "babel-loader": "8.2.3",
    "bootstrap": "4.5.3",
    "capybara-router": "2.6.0",
    "classnames": "2.3.1",
    "compression-webpack-plugin": "4.0.0",
    "concurrently": "7.0.0",
